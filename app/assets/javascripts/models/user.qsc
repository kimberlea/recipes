class Dishfave.User extends QS.Model
  @includeCollection()
  @includeAdapter
    load_url: '/users'
    save_url: '/user'
  init : ->
    @addFields ['first_name', 'last_name', 'picture_url'], ''
    @addFields ['chefscore', 'dishes_count', 'followers_count', 'followings_count'], 0
    @addFields ['flags'], []

    @addSubModel 'following', Dishfave.Following, true
